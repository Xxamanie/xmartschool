# Production Backend Environment Variables
# Copy this file to .env.production and fill in actual values
# Store production values in Render dashboard environment variables

# Database connection string (Render PostgreSQL)
DATABASE_URL=postgresql://user:password@dpg-xxxxx.onrender.com:5432/dbname

# Server port
PORT=4000

# Node environment
NODE_ENV=production

# Frontend origin (update to your Supabase-enabled frontend URL)
FRONTEND_URL=https://your-frontend-domain.com

# Supabase (server) credentials
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Gemini API Key (optional - for AI features)
GEMINI_API_KEY=
